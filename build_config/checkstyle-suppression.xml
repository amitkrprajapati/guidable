<?xml version="1.0"?>

<!DOCTYPE suppressions PUBLIC
        "-//Checkstyle//DTD SuppressionFilter Configuration 1.2//EN"
        "https://checkstyle.org/dtds/suppressions_1_2.dtd">
<suppressions>
    <suppress checks="JavadocMethod" files=".*" />
    <suppress checks="JavadocPackage" files=".*" />
    <suppress checks="SummaryJavadoc" files=".*" />
    <suppress checks="AbbreviationAsWordInName" files=".*" />
    <suppress checks="LineLength" files=".*" />
    <suppress checks="EmptyLineSeparator" files=".*" />
    <suppress checks="RightCurlyAlone" files=".*" />

</suppressions>